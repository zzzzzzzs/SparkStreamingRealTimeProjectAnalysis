<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper SYSTEM "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.me.gmallpublisher.mapper.DauMapper">

    <select id="selectDauTotal" resultType="Integer">
        select count(*) from GMALL1116_DAU where  logdate=#{date}
    </select>

    <select id="selectDauTotalHourMap" resultMap="dauTotalHour">
        select LOGHOUR lh, count(*) ct from GMALL1116_DAU where  LOGDATE=#{date}
        group by LOGHOUR
    </select>

    <resultMap id="dauTotalHour" type="java.util.Map" autoMapping="true">
    </resultMap>
</mapper>
